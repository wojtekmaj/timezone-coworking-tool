{
  "name": "timezone-coworking-tool",
  "version": "1.0.0",
  "description": "A tool to help with coworking with people in different timezones",
  "scripts": {
    "build": "parcel build src/index.html --public-url ./ --cache-dir ./.cache/prod --no-source-maps",
    "clean": "rimraf dist",
    "lint": "eslint src/ --ext .jsx,.js",
    "start": "parcel src/index.html --cache-dir ./.cache/dev",
    "test": "yarn lint"
